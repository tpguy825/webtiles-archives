}

button {
  color: #fff;
  cursor: pointer;
  background-color: #000;
  border: 1px solid #fff;
  border-radius: 4px;
  transition: background-color .2s, color .2s;
}

a {
  color: pink;
  font-size: 12px;
  text-decoration: none;
  transition: color .2s;
}

a:hover {
  color: #0ff;
