    <script type="text/tilescript">function request(method, url, callback) {
  var xhr = new XMLHttpRequest();

  xhr.open(method, url, true);

  xhr.onload = function () {
    if (xhr.readyState === 4) {
      callback(xhr.status, xhr.responseText);
    }
  };

  xhr.onerror = function () {
    callback(0, null);
  };

  xhr.send(null);
}</script>
