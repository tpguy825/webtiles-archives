    <script type="text/tilescript">
    	var logoN = document.getElementById("logo-n");
    	var kirby = document.getElementById("kirby");
    	var onlineStatus = document.getElementById("online-status");
        var currentSong = document.getElementById("current-song");
        var songName = document.getElementById("song-name");
        var hits = document.getElementById("hits");
        var visitors = document.getElementById("visitors");
        var lilMoons = document.getElementsByClassName("lil-moon");
         
        function checkOnlineStatus() {
            request("GET", "https://dimden.dev/services/slave/status/", function(code, text) {
                if(code !== 200) return;
                var json = JSON.parse(text);
                if(json.dimden === "online") {
                    onlineStatus.innerText = "Online!";
                    onlineStatus.style.color = "lime";
                } else {
                    onlineStatus.innerText = "Offline!";
                    onlineStatus.style.color = "red";
                }
            });
        }
        function checkSong() {
            request("GET", "https://dimden.dev/services/nowplaying", function(code, text) {
                if(code !== 200) return;
                var json = JSON.parse(text);
                currentSong.innerText = (json['@attr'] && json['@attr'].nowplaying === 'true') ? 'Current song: ' : 'Last song: ';
                songName.innerText = json.artist['#text'] + ' - ' + json.name;
            });
        }
        function checkCounter() {
            request("GET", "https://dimden.dev/tiles/counter/get", function(code, text) {
                if(code !== 200) return;
                var json = JSON.parse(text);
                hits.innerText = json.counter.toString();
                visitors.innerText = json.visitors.toString();
            });
        }
        
        checkOnlineStatus();
        checkSong();
        checkCounter();
        setInterval(function() {
         	checkOnlineStatus();
            checkSong();
            checkCounter();
        }, 20000);
        
        logoN.addEventListener("click", function() {
           	logoN.style.transform = "translate(0, 120px) rotate(20deg)"; 
        });
        if(!embedded) {
            request("GET", "https://dimden.dev/tiles/counter/count", function(code, text) {});
        } else {
            document.getElementById("eyeborder").hidden = true;
            document.getElementById("border-left").hidden = true;
            document.getElementById("border-top").hidden = true;
            document.getElementById("border-bottom-right").hidden = true;
        }
    </script>
