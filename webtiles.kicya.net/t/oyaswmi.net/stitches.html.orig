      <script type="text/tilescript">// Only executes while your tile is active
// Only ES5 is supported, limited subset of APIs - DOM, localStorage, XMLHttpRequest, Canvas.

var mouseOverPopup = false
var mouseOverTaskbar = false

function checkHover(){
    
    if (mouseOverTaskbar == true){
        document.getElementById("select").src = "https://webtiles.kicya.net/t/oyaswmi.net/desktop_select.png"
    }else{
        document.getElementById("select").src = "https://webtiles.kicya.net/t/oyaswmi.net/desktop.png"
    }
    
    if (mouseOverPopup == true || mouseOverTaskbar == true) {
          
    	document.getElementById("popup").style.opacity = "1";
        
        if (window.location.href != "/t/oyaswmi.net/stitches.html") {
            document.getElementById("clickMe").innerText = "Welcome To The Wired"
        }
    	
        document.getElementById("popup").style.top = "0px";
    }else {
   
    	document.getElementById("popup").style.opacity = "0";    
        document.getElementById("popup").style.top = "250px";

    }
}

document.getElementById("popup").onmouseover = function(){
	mouseOverPopup = true
    checkHover()
};

document.getElementById("popup").onmouseout = function(){
	mouseOverPopup = false
    checkHover()
};



document.getElementById("select").onmouseover = function(){
	mouseOverTaskbar = true
    checkHover()
};

document.getElementById("select").onmouseout = function(){
	mouseOverTaskbar = false
    checkHover()
};

</script>
