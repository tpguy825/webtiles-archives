  background-color: #000;
  background-image: url("/t/eyeorb.net/madotsuki_sprite.jpg");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100px;
  border: none;
  outline: none;
}

#btn:active {
  opacity: .5;
  background-image: url("/t/eyeorb.net/madotsuki_sprite.jpg");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 110px;
}

a {
  color: #d65367;
}
